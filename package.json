    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "22.4.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "jest": "22.4.3",
    "react-test-renderer": "15.4.2"
  },
  "jest": {
    "preset": "react-native"
  }
